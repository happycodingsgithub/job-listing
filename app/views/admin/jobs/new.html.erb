<h1> Add a job </h1>

<%= simple_form_for [:admin,@job] do |f| %>

<%= f.input :title, :label => "公司名称" %>
<%= f.input :description, :label => "行业" %>
<%= f.input :wage_upper_bound, :label => "上年度收入（单位万元）" %>
<%= f.input :wage_lower_bound, :label => "上年度纳税（单位万元）" %>
<%= f.input :contact_email, :label => "联系人" %>
<%= f.input :contact_email, :label => "联系人电话" %>
<%= f.input :is_hidden %>


  <%= f.submit "Submit" %>
<% end %>
