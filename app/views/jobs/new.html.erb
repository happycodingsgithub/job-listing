<h1> 增加一个社群 </h1>

<%= simple_form_for @job do |f| %>

<%= f.input :title %>
<%= f.input :description %>
<%= f.input :wage_lower_bound, :label => "已经在社群活跃频率下限" %>
<%= f.input :wage_upper_bound, :label => "已经在社群活跃频率上限" %>
<%= f.input :contact_email %>
<%= f.input :is_hidden %>


  <%= f.submit "提交" %>
<% end %>
