

<div class="pull-right">
  <%= link_to("提交企业资料", new_job_path, :class => "btn btn-default" ) %>
</div>

<table class="table table-boldered">
    <thead>
        <tr>
            <td>
                企业行业</td>
            <td>上年度收入（单位万元）</td>
            <td>上年度纳税（单位万元）</td>
            <td>提交时间</td>
        </tr>

    </thead>
    <tbody>
        <% @jobs.each do |job| %>
        <tr>
            <td>
                <%= job.description %>
            </td>
            <td>
                <%= job.wage_upper_bound  %>
            </td>
            <td>
                <%= job.wage_lower_bound%>
            </td>
            <td>
                <%= job.created_at %>
            </td>
        </tr>
        <% end %>
    </tbody>

</table>
